$(document).ready ->
    test = "Test"
    console.log (<%= 'test' %>)
    map = L.map('mapid', {
        maxZoom: 24,
        minZoom: 1,
        crs: L.CRS.Simple
    }).setView([51.505, -0.09], 2);
    map.setMaxBounds(new L.LatLngBounds([0,500], [500,0]));

    imageUrl = "<%= asset_path('SherlockHolmesMap.png') %>"
    imageBounds = [[250,0], [0,250]];
    L.imageOverlay(imageUrl, imageBounds).addTo(map);
  $(document).tooltip({
    position: {
      my: "center bottom",
      at: "center top",
    }
  })
